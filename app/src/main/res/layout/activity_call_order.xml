<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="pk.getsub.www.getsub.map.CallOrderActivity">

    <include layout="@layout/order_toolbar"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_height="wrap_content"
        tools:layout_constraintLeft_creator="1"
        android:layout_width="0dp"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/txt_first_call_order"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="03328469195"
        android:textSize="26sp"
        app:layout_constraintRight_toLeftOf="@+id/btn_first_call_order"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/btn_first_call_order"
        android:layout_marginEnd="38dp" />

    <Button
        android:id="@+id/btn_first_call_order"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:background="@color/colorPrimary"
        android:text="Call"
        android:textColor="#ffffff"
        android:textStyle="bold"
        android:layout_marginRight="32dp"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintTop_creator="1"
        android:layout_marginEnd="32dp"
        android:layout_marginTop="108dp"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_second_call_order"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="03328469195"
        android:textSize="26sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/txt_first_call_order"
        android:layout_marginTop="57dp"
        app:layout_constraintTop_toBottomOf="@+id/txt_first_call_order"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/txt_first_call_order" />

    <Button
        android:id="@+id/btn_second_call_order"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:background="@color/colorPrimary"
        android:text="Call"
        android:textColor="#ffffff"
        android:textStyle="bold"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/btn_first_call_order"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_first_call_order"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/btn_first_call_order" />
</android.support.constraint.ConstraintLayout>
